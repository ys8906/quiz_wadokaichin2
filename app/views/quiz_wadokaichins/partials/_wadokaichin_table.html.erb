<quiz-wadokaichin-show
  :quiz="<%= @quiz.to_json %>"
  v-bind:meanings="[<%= @quiz.jukugo_right.meaning.to_json %>,
                    <%= @quiz.jukugo_bottom.meaning.to_json %>,
                    <%= @quiz.jukugo_left.meaning.to_json %>,
                    <%= @quiz.jukugo_top.meaning.to_json %>]"
  :is-signed-in="<%= user_signed_in? ? true : false %>"
  :has-savedata="<%= @savedata.present? ? true : false %>"
  :url="<%= request.url.to_json %>"
></quiz-wadokaichin-show>
